{
  "$schema": "https://json.openmrs.org/routes.schema.json",
  "backendDependencies": {
      "fhir2": "^1.2.0",
      "webservices.rest": "^2.24.0"
  },
  "pages": [
    {
      "component": "facilityDashboard",
      "route": "facility-dashboard"
    },
    {
      "component": "hieDashboard",
      "route": "hie-dashboard"
    },
    {
      "component": "systemInfoPage",
      "route": "about"
    }
  ],
  "extensions": [
    {
      "name": "facility-home-dashboard-link",
      "component": "facilityHomeDashboardLink",
      "slot": "homepage-dashboard-slot",
      "privilege":[
        ""
      ],
      "meta": {
        "name": "facility-home-dashboard",
        "slot": "facility-home-dashboard-slot",
        "title": "Facility Dashboard"
      }
    },
    {
      "name": "facility-home-dashboard-ext",
      "component": "facilityHomeDashboardExt",
      "slot": "facility-home-dashboard-slot"
    },
    {
      "name": "hie-home-dashboard-link",
      "component": "hieHomeDashboardLink",
      "slot": "homepage-dashboard-slot",
      "privilege":[
        "View:hiedashboardui.home"
      ],
      "meta": {
        "name": "hie-home-dashboard",
        "slot": "hie-home-dashboard-slot",
        "title": "HIE Dashboard"
      }
    },
    {
      "name": "hie-dashboard-ext",
      "component": "hieHomeDashboardExt",
      "slot": "hie-home-dashboard-slot"
    },
    {
      "name": "clinical-views-divider",
      "component": "clinicalViewsDivider",
      "slot": "patient-chart-dashboard-slot",
      "order": 12
    },
    {
      "component": "cervicalCancerSummaryExt",
      "name": "cervical-cancer-summary-ext",
      "slot": "cacx-visits-slot",
      "meta": {
        "columnSpan": 4
      }
    },
    {
      "name": "system-info-link",
      "slot": "app-menu-slot",
      "component": "systemInfoMenuLink"
    },
    {
      "name": "retrieve-facility-code-modal",
      "component": "retrieveFacilityCodeModal"
    }
  ]
}

