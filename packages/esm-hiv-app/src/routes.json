{
  "$schema": "https://json.openmrs.org/routes.schema.json",
  "backendDependencies": {
    "fhir2": "^1.2.0",
    "webservices.rest": "^2.24.0"
  },
  "pages": [],
  "extensions": [
    {
      "component": "hivDashboardGroup",
