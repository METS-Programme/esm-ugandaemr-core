  },
  "dependencies": {
    "@carbon/react": "^1.64.1",
    "lodash-es": "^4.17.15",
    "react-to-print": "^2.14.13"
  },
  "peerDependencies": {
    "@openmrs/esm-framework": "*",
    "@openmrs/esm-patient-common-lib": "x",
    "react": "^18.1.0",
    "react-i18next": "11.x",
    "react-router-dom": "6.x",
    "swr": "2.x"
  },
  "devDependencies": {
    "@openmrs/esm-patient-common-lib": "next",
    "webpack": "^5.74.0"
  }
